                <span class="tabs-item__text">
                    <a class="nav-link text-dark" asp-area="" asp-page="/Account/Index">Account</a>
                </span>
            </li>
        </ul>
        <div class="tabs-content">

            <div class="tabs-content__item active">
                <div class="form-wrapper">
                    <form action="" class="form_order">
                        <div class="form-item">
                            <label for="id">Id</label>
                            <input type="text" class="form-item_input" id="id" />
                        </div>
                        <div class="form-item">
                            <label for="Name">Name</label>
                            <input type="text" class="form-item_input" id="Name" />
                        </div>
                        <div class="form-item">
                            <label for="Address">Address</label>
                            <input type="text" class="form-item_input" id="Address" />
                        </div>
                        <div class="form-item">
                            <label for="Phone">Phone</label>
                            <input type="text" class="form-item_input" id="Phone" />
                        </div>
                        <div class="form-item">
                            <label for="Email">Email</label>
                            <input type="text" class="form-item_input" id="Email" />
                        </div>
                    </form>
                    <div class="list-btn">
                        <button class="btn">Add</button>
                        <button class="btn">Update</button>
                        <button class="btn">Delete</button>
                        <button class="btn">ViewWork</button>
                    </div>
                    <div class="search right">
                        <div class="search-input right">
                            <img src="./assets/images/search.svg"
                                 alt=""
                                 class="search-icon" />
                            <input type="text" placeholder="Search" />
                        </div>
                    </div>
                    <div class="table">
                        <table>
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Phone</th>
                                <th>Email</th>
                            </tr>
                            <tr>
                                <td>1</td>
